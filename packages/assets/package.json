{
  "name": "@quajs/assets",
  "type": "module",
  "version": "0.1.0",
  "description": "Browser-based asset manager for QuaEngine - loads and manages Quack bundles with IndexedDB storage",
  "author": "QuaDevTeam",
  "license": "Apache-2.0",
  "keywords": [
    "quaengine",
    "assets",
    "game-engine",
    "indexeddb",
    "bundle-manager",
    "browser",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@quajs/logger": "workspace:*"
  },
  "dependencies": {
    "dexie": "^4.0.8",
    "fflate": "^0.8.2",
    "lzma-web": "^3.0.1"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "browser": true
}
